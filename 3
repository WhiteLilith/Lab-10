using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace SoloLearn
{
    class Program
    {
        static void Main(string[] args)
        {
            int N, a, i, s;
            Console.Write("Введите число для нахождения его квадрата: ");
            N = int.Parse(Console.ReadLine());
            s = 0;
            for (i = 1; i <= N; i++)
            {
                a = (i * 2) - 1;
                s = s + a;
                Console.WriteLine(s);
            }
            Console.ReadKey();
        }
    }
}
